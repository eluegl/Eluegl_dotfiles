super + @space
        rofi -show run 
 
super + f 
        firefox 
 
super + t 
        kitty 
 
super + c 
        bspc node -c 
 
super + r 
        bspc wm -r 
 
super + {shift + t,shift +f} 
        bspc node -t {tiled,fullscreen} 
 
super + {_,shift + }{q,s,z,d} 
        bspc node -{f,s} {west,south,north,east} 
 
super + {F1-F10} 
        bspc desktop -f {1-9} 
 
super + shift + {F1-F10} 
        bspc node -d {1-9} 
 
super + ctrl + {q,s,z,d} 
        bspc node -z {left -80 0,bottom 0 80, top 0 -80, right 80 0} 
 
super + ctrl + shift + {q,s,z,d} 
        bspc node -z {right -80 0,top 0 80,bottom 0 -80,left 80 0} 
 
super + l 
        slock 
 
super + Print 
        scrot -s -F ~/img/screenshots/%Y-%m-%d-%H%M%S.png 
 
super + ctrl + f 
        flatpak run dev.goats.xivlauncher 
 
super + Left 
        bspc desktop -f prev 
 
super + Right 
        bspc desktop -f next 
 
XF86AudioRaiseVolume 
    wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%+ 
 
XF86AudioLowerVolume 
    wpctl set-volume @DEFAULT_AUDIO_SINK@ 5%- 
 
XF86AudioMute 
    wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle 
 
XF86AudioPlay 
    playerctl play-pause 
 
XF86AudioNext 
    playerctl next 
 
XF86AudioPrev 
    playerctl previous
